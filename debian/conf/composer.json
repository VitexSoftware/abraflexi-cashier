{
  "name": "deb/abraflexi-cashier",
  "description": "Every week, he inserts an expense document \"own consumption\" into the cash register so that the cash register remains empty in accounting terms.",
  "type": "project",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "config": {
    "vendor-dir": "/var/lib/composer/abraflexi-cashier"
  },
  "require": {
    "deb/abraflexi": "*",
    "deb/ease-core": "*",
    "deb/abraflexi-bricks": "*"
  },
  "autoload": {
    "psr-4": {
      "AbraFlexi\\Cash\\": "./"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/EaseCore"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexi"
    }
  ]
}
